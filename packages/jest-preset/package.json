{
  "name": "jest-preset-beemo",
  "version": "0.2.1",
  "description": "Jest preset for Beemo configured projects.",
  "keywords": [
    "jest-preset",
    "beemo"
  ],
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "jest-preset.js",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:beemojs/dev.git",
    "directory": "packages/jest-preset"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  },
  "peerDependencies": {
    "jest": ">=26.0.0"
  },
  "dependencies": {
    "@beemo/config-constants": "^0.2.0",
    "@types/jest": "^26.0.23",
    "jest-circus": "^27.0.4"
  },
  "devDependencies": {
    "jest": "^27.0.4"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "fileMock": "src/fileMock.ts"
    },
    "format": "lib",
    "platform": "node"
  }
}
