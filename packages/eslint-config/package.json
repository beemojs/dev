{
  "name": "eslint-config-beemo",
  "version": "0.0.1-alpha.0",
  "description": "Reusable ESLint config for Beemo configured projects.",
  "keywords": [
    "eslint-config",
    "airbnb",
    "beemo",
    "react",
    "typescript"
  ],
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "browser.js",
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "node.js",
    "react.js",
    "src/**/*.{ts,tsx,json}"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:beemojs/dev.git",
    "directory": "packages/eslint-config"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  },
  "peerDependencies": {
    "eslint": "^7.0.0"
  },
  "dependencies": {
    "@beemo/config-constants": "^0.0.1-alpha.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^29.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.10",
    "eslint": "^7.21.0"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "async": "src/async.ts",
      "browser": "src/browser.ts",
      "module": "src/module.ts",
      "node": "src/node.ts",
      "react": "src/react.ts",
      "tests": "src/tests.ts",
      "typescript": "src/typescript.ts",
      "unicorn": "src/unicorn.ts"
    },
    "platform": "node"
  }
}
